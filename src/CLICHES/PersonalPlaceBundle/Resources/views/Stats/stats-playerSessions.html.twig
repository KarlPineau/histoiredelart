{% if playerSessionCollection['playerOeuvres'] is not empty %}
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="heading{{ loop.index }}">
                <h3 class="panel-title">
                    <span class="caret"></span>
                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse{{ loop.index }}" aria-expanded="true" aria-controls="collapse{{ loop.index }}">
                    Partie du {{ playerSessionCollection['playerSession'].dateBegin|date("d-m-Y") }} ({{ cliches_player_playersession_service.getNumberCliches(playerSessionCollection['playerSession']) }} clich√©s)
                        {% if playerSessionCollection['playerSession'].simpleSession == true %} - Mode examen{% endif %}
                    </a>
                </h3>
            </div>
            <div id="collapse{{ loop.index }}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading{{ loop.index }}">
                <div class="panel-body">
                    {% for playerOeuvreCollection in playerSessionCollection['playerOeuvres'] %}
                        {% include "CLICHESPersonalPlaceBundle:Stats:stats-playerOeuvres.html.twig" %}
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
{% endif %}