{# src/DATA/AdministrationBundle/Resources/views/layout.html.twig #}
 
{% extends "TBLayoutBundle::layout.html.twig" %}

{% block title %}
    Administration - {{ parent() }}
{% endblock %}

{% block follow %}
    <meta name="robots" content="noindex,nofollow" />
{% endblock %}

{% block h1 %}{% endblock %}

{% block h1_extend %}{% endblock %}

{% block breadcrumb %}
    {{parent()}}
    {% if is_granted("ROLE_ADMIN") %}<li><a href="{{ path('tb_administration_home_index') }}">Administration</a></li>
    {% else %}<li><a href="{{ path('tb_personalplace_home_index') }}">Profil</a></li>
    {% endif %}
{% endblock %}

{% block content %}{% endblock %}

{% block javascript %}
    {{parent()}}
    {% block javascript_sub %}
    {% endblock %}
{% endblock %}