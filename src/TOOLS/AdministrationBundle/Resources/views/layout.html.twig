{# src/DATA/AdministrationBundle/Resources/views/layout.html.twig #}
 
{% extends "TOOLSLayoutBundle::layout.html.twig" %}

{% block title %}
    Administration - {{ parent() }}
{% endblock %}

{% block follow %}
    <meta name="robots" content="noindex,nofollow" />
{% endblock %}

{% block h1 %}{% endblock %}

{% block h1_extend %}{% endblock %}

{% block breadcrumb %}
    {{parent()}}
    {% if app.request.get('_route') == 'tools_administration_home_index' %}<li class="active">Administration</li>
    {% else %}<li><a href="{{ path('tools_administration_home_index') }}">Administration</a></li>
    {% endif %}
{% endblock %}

{% block content %}{% endblock %}

{% block javascript %}
    {{parent()}}
    {% block javascript_sub %}
    {% endblock %}
{% endblock %}